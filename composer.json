{
    "name": "tripsome/blog",
    "description": "Blog Publishing Framework.",
    "keywords": ["framework", "laravel", "blog"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Said",
            "email": "mohamed@laravel.com"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "laravel/framework": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Tripsome\\Blog\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Tripsome\\Blog\\BlogServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
